<% @restaurants.each do |restaurant| %>

    <div class="col-4">
    <%= image_tag restaurant.photo_url(:thumbnail).to_s %>
    <h3 class="h5 mt-3"><%= restaurant.name %></h3>
    <p><%= restaurant.address %></p>
    <%= button_to "Book Restaurant", restaurant_path(restaurant), class: "btn btn-primary navbar-btn ml-0 ml-lg-3" %>
    </div>

<% end %>