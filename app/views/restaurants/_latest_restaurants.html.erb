<% @restaurants.each do |restaurant| %>

    <div class="col-4">
    <%= link_to image_tag restaurant.photo_url(:thumbnail).to_s, restaurant_path(restaurant) %>
    <h3 class="h5 mt-3"><%= restaurant.name %></h3>
    <p><%= restaurant.address %></p>
    <p><%= restaurant.cuisine %></p>
    </div>

<% end %>